{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "Tools for visualizing entities and explosions",
  "authors": [
    "lntricate"
  ],
  "contact": {
    "sources": "https://github.com/lntricate1/EntityVisualizer"
  },

  "icon": "assets/entityvisualizer/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.lntricate.entityvisualizer.EntityVisualizerMod"
    ],
    "preLaunch": [
      "me.lntricate.entityvisualizer.PreLaunch"
    ],
    "modmenu": [
      "me.lntricate.entityvisualizer.ModMenuImpl"
    ]
  },
  "mixins": [
    "entityvisualizer.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}"
  }
}
